    "from sympy import Symbol, exp, sqrt, pi, Integral, S\n",
    "\n",
    "x = Symbol('x')\n",
    "p = exp(-(x - 10)**2/2)/sqrt(2*pi)\n",
    "\n",
    "Integral(p, (x, S.NegativeInfinity, S.Infinity)).doit().evalf()\n"
   ]
